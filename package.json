{
  "name": "opendocumenter",
  "version": "0.4.1",
  "main": "index.js",
  "license": "MIT",
  "index": "src/index.js",
  "author": "$ourOpenCode",
  "bin": {
    "opendocumenter": "src/cli.js"
  },
  "scripts": {
    "start": "npm-run-all dev-generate dev-serve",
    "dev": "nodemon -e js,vue,less,sass,css,html,json,yml --ignore tmp/ --ignore example/docs/ --exec npm start",
    "dev-generate": "node ./src/cli.js --schema=./example/openapi.yml --output=./example/docs --config=./example/config.json",
    "dev-serve": "serve ./example/docs",
    "dev-nuxt": "cd src-nuxt && nuxt"
  },
  "engines": {
    "node": ">=14.6"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.1",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@nuxtjs/markdownit": "^1.2.10",
    "@nuxtjs/router": "^1.5.0",
    "babel-runtime": "^6.26.0",
    "colors": "^1.4.0",
    "highlight.js": "10.1.2",
    "less-loader": "^6.2.0",
    "markdown-it-highlightjs": "^3.2.0",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "nuxt": "^2.14.0",
    "nuxt-fontawesome": "^0.4.0",
    "nuxt-webfontloader": "^1.1.0",
    "recursive-copy": "^2.0.10",
    "uuid": "^8.3.0",
    "vue-highlight.js": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "serve": "^11.3.2"
  }
}
